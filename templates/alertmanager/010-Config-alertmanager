global:
  resolve_timeout: 5m
route:
  group_by: ['alertname']
  group_wait: 60s
  group_interval: 60s
  repeat_interval: 1h
  receiver: 'default-receiver'
receivers:
- name: 'default-receiver'
inhibit_rules:
  - source_match:
      severity: 'critical'
    target_match:
      severity: 'warning'
    equal: ['alertname']