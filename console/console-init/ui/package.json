{
  "name": "patternfly-seed",
  "version": "0.0.1",
  "description": "An open source build scaffolding utility for web apps.",
  "main": "index.js",
  "repository": "https://github.com/patternfly/patternfly-react-seed.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src/",
    "build:bundle-profile": "webpack --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "yarn rimraf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@patternfly/react-core": "^2.10.9",
    "axios-mock-adapter": "^1.16.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^4.19.1",
    "eslint-plugin-patternfly-react": "^0.2.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^6.x",
    "img-loader": "^3.0.1",
    "jest": "^24.7.1",
    "lodash": "^4.x",
    "mini-css-extract-plugin": "^0.4.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-axe": "^3.0.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "redux": "^3.x",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@patternfly/patternfly": "^1.0.250",
    "@patternfly/react-table": "^1.4.10",
    "@zalando/oauth2-client-js": "^0.0.18",
    "axios": "^0.18.0",
    "moment": "^2.24.0"
  }
}
