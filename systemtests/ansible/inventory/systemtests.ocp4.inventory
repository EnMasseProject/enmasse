[enmasse]
localhost ansible_connection=local

[enmasse:vars]
namespace=enmasse-infra
enable_rbac=True
api_server=True
register_api_server=True
service_catalog=False
secure_api_server=True
keycloak_admin_password=admin
authentication_services=["none", "standard"]
standard_authentication_service_postgresql=False
monitoring_infrastructure=False
service_monitors=False
enable_iot=False
alerting=False
